@prefix : <http://www.example.org/disease-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@base <http://www.example.org/disease-ontology#> .

<http://www.example.org/disease-ontology> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.example.org/disease-ontology#name
:name rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.example.org/disease-ontology#exhibitsSymptom
:exhibitsSymptom rdf:type owl:ObjectProperty ;
                 rdfs:domain :Patient ;
                 rdfs:range :Symptom .


###  http://www.example.org/disease-ontology#hasDisease
:hasDisease rdf:type owl:ObjectProperty ;
            rdfs:domain :Patient ;
            rdfs:range :Disease .


###  http://www.example.org/disease-ontology#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:domain :Disease ;
            rdfs:range :Symptom .


###  http://www.example.org/disease-ontology#hasTreatment
:hasTreatment rdf:type owl:ObjectProperty ;
              rdfs:domain :Disease ;
              rdfs:range :Treatment .


###  http://www.example.org/disease-ontology#receivesTreatment
:receivesTreatment rdf:type owl:ObjectProperty ;
                   rdfs:domain :Patient ;
                   rdfs:range :Treatment .


###  http://www.example.org/disease-ontology#tratamento
:tratamento rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.example.org/disease-ontology#descrição
:descrição rdf:type owl:DatatypeProperty ;
           rdfs:domain :Disease .


#################################################################
#    Classes
#################################################################

###  http://www.example.org/disease-ontology#Disease
:Disease rdf:type owl:Class .


###  http://www.example.org/disease-ontology#Patient
:Patient rdf:type owl:Class .


###  http://www.example.org/disease-ontology#Symptom
:Symptom rdf:type owl:Class .


###  http://www.example.org/disease-ontology#Treatment
:Treatment rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.example.org/disease-ontology#AIDS
:AIDS rdf:type owl:NamedIndividual ,
               :Disease ;
      :hasSymptom :extra_marital_contacts ,
                  :high_fever ,
                  :muscle_wasting ,
                  :patches_in_throat ;
      :descrição "Acquired immunodeficiency syndrome (AIDS) is a chronic, potentially life-threatening condition caused by the human immunodeficiency virus (HIV). By damaging your immune system, HIV interferes with your body's ability to fight infection and disease." .


###  http://www.example.org/disease-ontology#Acne
:Acne rdf:type owl:NamedIndividual ,
               :Disease ;
      :hasSymptom :blackheads ,
                  :pus_filled_pimples ,
                  :scurring ,
                  :skin_rash ;
      :descrição "Acne vulgaris is the formation of comedones, papules, pustules, nodules, and/or cysts as a result of obstruction and inflammation of pilosebaceous units (hair follicles and their accompanying sebaceous gland). Acne develops on the face and upper trunk. It most often affects adolescents." .


###  http://www.example.org/disease-ontology#Alcoholic_hepatitis
:Alcoholic_hepatitis rdf:type owl:NamedIndividual ,
                              :Disease ;
                     :hasSymptom :abdominal_pain ,
                                 :distention_of_abdomen ,
                                 :fluid_overload ,
                                 :history_of_alcohol_consumption ,
                                 :swelling_of_stomach ,
                                 :vomiting ,
                                 :yellowish_skin ;
                     :descrição "Alcoholic hepatitis is a diseased, inflammatory condition of the liver caused by heavy alcohol consumption over an extended period of time. It's also aggravated by binge drinking and ongoing alcohol use. If you develop this condition, you must stop drinking alcohol" .


###  http://www.example.org/disease-ontology#Allergy
:Allergy rdf:type owl:NamedIndividual ,
                  :Disease ;
         :hasSymptom :chills ,
                     :continuous_sneezing ,
                     :shivering ,
                     :watering_from_eyes ;
         :descrição "An allergy is an immune system response to a foreign substance that's not typically harmful to your body.They can include certain foods, pollen, or pet dander. Your immune system's job is to keep you healthy by fighting harmful pathogens." .


###  http://www.example.org/disease-ontology#AntiviralDrugs
:AntiviralDrugs rdf:type owl:NamedIndividual ,
                         :Treatment .


###  http://www.example.org/disease-ontology#Arthritis
:Arthritis rdf:type owl:NamedIndividual ,
                    :Disease ;
           :hasSymptom :movement_stiffness ,
                       :muscle_weakness ,
                       :painful_walking ,
                       :stiff_neck ,
                       :swelling_joints ;
           :descrição "Arthritis is the swelling and tenderness of one or more of your joints. The main symptoms of arthritis are joint pain and stiffness, which typically worsen with age. The most common types of arthritis are osteoarthritis and rheumatoid arthritis." .


###  http://www.example.org/disease-ontology#Bronchial_Asthma
:Bronchial_Asthma rdf:type owl:NamedIndividual ,
                           :Disease ;
                  :hasSymptom :breathlessness ,
                              :cough ,
                              :family_history ,
                              :fatigue ,
                              :high_fever ,
                              :mucoid_sputum ;
                  :descrição "Bronchial asthma is a medical condition which causes the airway path of the lungs to swell and narrow. Due to this swelling, the air path produces excess mucus making it hard to breathe, which results in coughing, short breath, and wheezing. The disease is chronic and interferes with daily working." .


###  http://www.example.org/disease-ontology#Cervical_spondylosis
:Cervical_spondylosis rdf:type owl:NamedIndividual ,
                               :Disease ;
                      :hasSymptom :back_pain ,
                                  :dizziness ,
                                  :loss_of_balance ,
                                  :neck_pain ,
                                  :weakness_in_limbs ;
                      :descrição "Cervical spondylosis is a general term for age-related wear and tear affecting the spinal disks in your neck. As the disks dehydrate and shrink, signs of osteoarthritis develop, including bony projections along the edges of bones (bone spurs)." .


###  http://www.example.org/disease-ontology#Chicken_pox
:Chicken_pox rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :fatigue ,
                         :headache ,
                         :high_fever ,
                         :itching ,
                         :lethargy ,
                         :loss_of_appetite ,
                         :malaise ,
                         :mild_fever ,
                         :red_spots_over_body ,
                         :skin_rash ,
                         :swelled_lymph_nodes ;
             :descrição "Chickenpox is a highly contagious disease caused by the varicella-zoster virus (VZV). It can cause an itchy, blister-like rash. The rash first appears on the chest, back, and face, and then spreads over the entire body, causing between 250 and 500 itchy blisters." .


###  http://www.example.org/disease-ontology#Chronic_cholestasis
:Chronic_cholestasis rdf:type owl:NamedIndividual ,
                              :Disease ;
                     :hasSymptom :abdominal_pain ,
                                 :itching ,
                                 :loss_of_appetite ,
                                 :nausea ,
                                 :vomiting ,
                                 :yellowing_of_eyes ,
                                 :yellowish_skin ;
                     :descrição "Chronic cholestatic diseases, whether occurring in infancy, childhood or adulthood, are characterized by defective bile acid transport from the liver to the intestine, which is caused by primary damage to the biliary epithelium in most cases" .


###  http://www.example.org/disease-ontology#Common_Cold
:Common_Cold rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :chest_pain ,
                         :chills ,
                         :congestion ,
                         :continuous_sneezing ,
                         :cough ,
                         :fatigue ,
                         :headache ,
                         :high_fever ,
                         :loss_of_smell ,
                         :malaise ,
                         :muscle_pain ,
                         :phlegm ,
                         :redness_of_eyes ,
                         :runny_nose ,
                         :sinus_pressure ,
                         :swelled_lymph_nodes ,
                         :throat_irritation ;
             :descrição "The common cold is a viral infection of your nose and throat (upper respiratory tract). It's usually harmless, although it might not feel that way. Many types of viruses can cause a common cold." .


###  http://www.example.org/disease-ontology#Cough
:Cough rdf:type owl:NamedIndividual ,
                :Symptom .


###  http://www.example.org/disease-ontology#Dengue
:Dengue rdf:type owl:NamedIndividual ,
                 :Disease ;
        :hasSymptom :back_pain ,
                    :chills ,
                    :fatigue ,
                    :headache ,
                    :high_fever ,
                    :joint_pain ,
                    :loss_of_appetite ,
                    :malaise ,
                    :muscle_pain ,
                    :nausea ,
                    :pain_behind_the_eyes ,
                    :red_spots_over_body ,
                    :skin_rash ,
                    :vomiting ;
        :descrição "an acute infectious disease caused by a flavivirus (species Dengue virus of the genus Flavivirus), transmitted by aedes mosquitoes, and characterized by headache, severe joint pain, and a rash. — called also breakbone fever, dengue fever." .


###  http://www.example.org/disease-ontology#Diabetes
:Diabetes rdf:type owl:NamedIndividual ,
                   :Disease ;
          :hasSymptom :Fatigue ,
                      :FrequentUrination ,
                      :IncreasedThirst ,
                      :blurred_and_distorted_vision ,
                      :excessive_hunger ,
                      :fatigue ,
                      :increased_appetite ,
                      :irregular_sugar_level ,
                      :lethargy ,
                      :obesity ,
                      :polyuria ,
                      :restlessness ,
                      :weight_loss ;
          :hasTreatment :DietModification ,
                        :Exercise ,
                        :InsulinTherapy ;
          :descrição "Diabetes is a disease that occurs when your blood glucose, also called blood sugar, is too high. Blood glucose is your main source of energy and comes from the food you eat. Insulin, a hormone made by the pancreas, helps glucose from food get into your cells to be used for energy." .


###  http://www.example.org/disease-ontology#DietModification
:DietModification rdf:type owl:NamedIndividual ,
                           :Treatment .


###  http://www.example.org/disease-ontology#Dimorphic_hemmorhoidspiles
:Dimorphic_hemmorhoidspiles rdf:type owl:NamedIndividual ,
                                     :Disease ;
                            :hasSymptom :bloody_stool ,
                                        :constipation ,
                                        :irritation_in_anus ,
                                        :pain_during_bowel_movements ,
                                        :pain_in_anal_region .


###  http://www.example.org/disease-ontology#Dimorphic_hemorrhoidspiles
:Dimorphic_hemorrhoidspiles rdf:type owl:NamedIndividual ;
                            :descrição "Hemorrhoids, also spelled haemorrhoids, are vascular structures in the anal canal. In their ... Other names, Haemorrhoids, piles, hemorrhoidal disease ." .


###  http://www.example.org/disease-ontology#Drug_Reaction
:Drug_Reaction rdf:type owl:NamedIndividual ,
                        :Disease ;
               :hasSymptom :burning_micturition ,
                           :itching ,
                           :skin_rash ,
                           :spotting__urination ,
                           :stomach_pain ;
               :descrição "An adverse drug reaction (ADR) is an injury caused by taking medication. ADRs may occur following a single dose or prolonged administration of a drug or result from the combination of two or more drugs." .


###  http://www.example.org/disease-ontology#Exercise
:Exercise rdf:type owl:NamedIndividual ,
                   :Treatment .


###  http://www.example.org/disease-ontology#Fatigue
:Fatigue rdf:type owl:NamedIndividual ,
                  :Symptom .


###  http://www.example.org/disease-ontology#Fever
:Fever rdf:type owl:NamedIndividual ,
                :Symptom .


###  http://www.example.org/disease-ontology#Flu
:Flu rdf:type owl:NamedIndividual ,
              :Disease ;
     :hasSymptom :Cough ,
                 :Fever ,
                 :SoreThroat ;
     :hasTreatment :AntiviralDrugs ,
                   :Hydration ,
                   :Rest .


###  http://www.example.org/disease-ontology#FrequentUrination
:FrequentUrination rdf:type owl:NamedIndividual ,
                            :Symptom .


###  http://www.example.org/disease-ontology#Fungal_infection
:Fungal_infection rdf:type owl:NamedIndividual ,
                           :Disease ;
                  :hasSymptom :dischromic__patches ,
                              :itching ,
                              :nodal_skin_eruptions ,
                              :skin_rash ;
                  :descrição "In humans, fungal infections occur when an invading fungus takes over an area of the body and is too much for the immune system to handle. Fungi can live in the air, soil, water, and plants. There are also some fungi that live naturally in the human body. Like many microbes, there are helpful fungi and harmful fungi." .


###  http://www.example.org/disease-ontology#GERD
:GERD rdf:type owl:NamedIndividual ,
               :Disease ;
      :hasSymptom :acidity ,
                  :chest_pain ,
                  :cough ,
                  :stomach_pain ,
                  :ulcers_on_tongue ,
                  :vomiting ;
      :descrição "Gastroesophageal reflux disease, or GERD, is a digestive disorder that affects the lower esophageal sphincter (LES), the ring of muscle between the esophagus and stomach. Many people, including pregnant women, suffer from heartburn or acid indigestion caused by GERD." .


###  http://www.example.org/disease-ontology#Gastroenteritis
:Gastroenteritis rdf:type owl:NamedIndividual ,
                          :Disease ;
                 :hasSymptom :dehydration ,
                             :diarrhoea ,
                             :sunken_eyes ,
                             :vomiting ;
                 :descrição "Gastroenteritis is an inflammation of the digestive tract, particularly the stomach, and large and small intestines. Viral and bacterial gastroenteritis are intestinal infections associated with symptoms of diarrhea , abdominal cramps, nausea , and vomiting ." .


###  http://www.example.org/disease-ontology#Heart_attack
:Heart_attack rdf:type owl:NamedIndividual ,
                       :Disease ;
              :hasSymptom :breathlessness ,
                          :chest_pain ,
                          :sweating ,
                          :vomiting ;
              :descrição "The death of heart muscle due to the loss of blood supply. The loss of blood supply is usually caused by a complete blockage of a coronary artery, one of the arteries that supplies blood to the heart muscle." .


###  http://www.example.org/disease-ontology#Hepatitis_B
:Hepatitis_B rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :abdominal_pain ,
                         :dark_urine ,
                         :fatigue ,
                         :itching ,
                         :lethargy ,
                         :loss_of_appetite ,
                         :malaise ,
                         :receiving_blood_transfusion ,
                         :receiving_unsterile_injections ,
                         :yellow_urine ,
                         :yellowing_of_eyes ,
                         :yellowish_skin ;
             :descrição "Hepatitis B is an infection of your liver. It can cause scarring of the organ, liver failure, and cancer. It can be fatal if it isn't treated. It's spread when people come in contact with the blood, open sores, or body fluids of someone who has the hepatitis B virus." .


###  http://www.example.org/disease-ontology#Hepatitis_C
:Hepatitis_C rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :family_history ,
                         :fatigue ,
                         :loss_of_appetite ,
                         :nausea ,
                         :yellowing_of_eyes ,
                         :yellowish_skin ;
             :descrição "Inflammation of the liver due to the hepatitis C virus (HCV), which is usually spread via blood transfusion (rare), hemodialysis, and needle sticks. The damage hepatitis C does to the liver can lead to cirrhosis and its complications as well as cancer." .


###  http://www.example.org/disease-ontology#Hepatitis_D
:Hepatitis_D rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :abdominal_pain ,
                         :dark_urine ,
                         :fatigue ,
                         :joint_pain ,
                         :loss_of_appetite ,
                         :nausea ,
                         :vomiting ,
                         :yellowing_of_eyes ,
                         :yellowish_skin ;
             :descrição "Hepatitis D, also known as the hepatitis delta virus, is an infection that causes the liver to become inflamed. This swelling can impair liver function and cause long-term liver problems, including liver scarring and cancer. The condition is caused by the hepatitis D virus (HDV)." .


###  http://www.example.org/disease-ontology#Hepatitis_E
:Hepatitis_E rdf:type owl:NamedIndividual ,
                      :Disease ;
             :hasSymptom :abdominal_pain ,
                         :acute_liver_failure ,
                         :coma ,
                         :dark_urine ,
                         :fatigue ,
                         :high_fever ,
                         :joint_pain ,
                         :loss_of_appetite ,
                         :nausea ,
                         :stomach_bleeding ,
                         :vomiting ,
                         :yellowing_of_eyes ,
                         :yellowish_skin ;
             :descrição "A rare form of liver inflammation caused by infection with the hepatitis E virus (HEV). It is transmitted via food or drink handled by an infected person or through infected water supplies in areas where fecal matter may get into the water. Hepatitis E does not cause chronic liver disease." .


###  http://www.example.org/disease-ontology#Hydration
:Hydration rdf:type owl:NamedIndividual ,
                    :Treatment .


###  http://www.example.org/disease-ontology#Hypertension
:Hypertension rdf:type owl:NamedIndividual ,
                       :Disease ;
              :hasSymptom :chest_pain ,
                          :dizziness ,
                          :headache ,
                          :lack_of_concentration ,
                          :loss_of_balance ;
              :descrição "Hypertension (HTN or HT), also known as high blood pressure (HBP), is a long-term medical condition in which the blood pressure in the arteries is persistently elevated. High blood pressure typically does not cause symptoms." .


###  http://www.example.org/disease-ontology#Hyperthyroidism
:Hyperthyroidism rdf:type owl:NamedIndividual ,
                          :Disease ;
                 :hasSymptom :abnormal_menstruation ,
                             :diarrhoea ,
                             :excessive_hunger ,
                             :fast_heart_rate ,
                             :fatigue ,
                             :irritability ,
                             :mood_swings ,
                             :muscle_weakness ,
                             :restlessness ,
                             :sweating ,
                             :weight_loss ;
                 :descrição "Hyperthyroidism (overactive thyroid) occurs when your thyroid gland produces too much of the hormone thyroxine. Hyperthyroidism can accelerate your body's metabolism, causing unintentional weight loss and a rapid or irregular heartbeat." .


###  http://www.example.org/disease-ontology#Hypoglycemia
:Hypoglycemia rdf:type owl:NamedIndividual ,
                       :Disease ;
              :hasSymptom :anxiety ,
                          :blurred_and_distorted_vision ,
                          :drying_and_tingling_lips ,
                          :excessive_hunger ,
                          :fatigue ,
                          :headache ,
                          :irritability ,
                          :nausea ,
                          :palpitations ,
                          :slurred_speech ,
                          :sweating ,
                          :vomiting ;
              :descrição "Hypoglycemia is a condition in which your blood sugar (glucose) level is lower than normal. Glucose is your body's main energy source. Hypoglycemia is often related to diabetes treatment. But other drugs and a variety of conditions — many rare — can cause low blood sugar in people who don't have diabetes." .


###  http://www.example.org/disease-ontology#Hypothyroidism
:Hypothyroidism rdf:type owl:NamedIndividual ,
                         :Disease ;
                :hasSymptom :abnormal_menstruation ,
                            :brittle_nails ,
                            :cold_hands_and_feets ,
                            :depression ,
                            :dizziness ,
                            :enlarged_thyroid ,
                            :fatigue ,
                            :irritability ,
                            :lethargy ,
                            :mood_swings ,
                            :puffy_face_and_eyes ,
                            :swollen_extremeties ,
                            :weight_gain ;
                :descrição "Hypothyroidism, also called underactive thyroid or low thyroid, is a disorder of the endocrine system in which the thyroid gland does not produce enough thyroid hormone." .


###  http://www.example.org/disease-ontology#Impetigo
:Impetigo rdf:type owl:NamedIndividual ,
                   :Disease ;
          :hasSymptom :blister ,
                      :high_fever ,
                      :red_sore_around_nose ,
                      :skin_rash ,
                      :yellow_crust_ooze ;
          :descrição "Impetigo (im-puh-TIE-go) is a common and highly contagious skin infection that mainly affects infants and children. Impetigo usually appears as red sores on the face, especially around a child's nose and mouth, and on hands and feet. The sores burst and develop honey-colored crusts." .


###  http://www.example.org/disease-ontology#IncreasedThirst
:IncreasedThirst rdf:type owl:NamedIndividual ,
                          :Symptom .


###  http://www.example.org/disease-ontology#InsulinTherapy
:InsulinTherapy rdf:type owl:NamedIndividual ,
                         :Treatment .


###  http://www.example.org/disease-ontology#Jaundice
:Jaundice rdf:type owl:NamedIndividual ,
                   :Disease ;
          :hasSymptom :abdominal_pain ,
                      :dark_urine ,
                      :fatigue ,
                      :high_fever ,
                      :itching ,
                      :vomiting ,
                      :weight_loss ,
                      :yellowish_skin ;
          :descrição "Yellow staining of the skin and sclerae (the whites of the eyes) by abnormally high blood levels of the bile pigment bilirubin. The yellowing extends to other tissues and body fluids. Jaundice was once called the morbus regius (the regal disease) in the belief that only the touch of a king could cure it" .


###  http://www.example.org/disease-ontology#Malaria
:Malaria rdf:type owl:NamedIndividual ,
                  :Disease ;
         :hasSymptom :chills ,
                     :diarrhoea ,
                     :headache ,
                     :high_fever ,
                     :muscle_pain ,
                     :nausea ,
                     :sweating ,
                     :vomiting ;
         :descrição "An infectious disease caused by protozoan parasites from the Plasmodium family that can be transmitted by the bite of the Anopheles mosquito or by a contaminated needle or transfusion. Falciparum malaria is the most deadly type." .


###  http://www.example.org/disease-ontology#Migraine
:Migraine rdf:type owl:NamedIndividual ,
                   :Disease ;
          :hasSymptom :acidity ,
                      :blurred_and_distorted_vision ,
                      :depression ,
                      :excessive_hunger ,
                      :headache ,
                      :indigestion ,
                      :irritability ,
                      :stiff_neck ,
                      :visual_disturbances ;
          :descrição "A migraine can cause severe throbbing pain or a pulsing sensation, usually on one side of the head. It's often accompanied by nausea, vomiting, and extreme sensitivity to light and sound. Migraine attacks can last for hours to days, and the pain can be so severe that it interferes with your daily activities." .


###  http://www.example.org/disease-ontology#Osteoarthristis
:Osteoarthristis rdf:type owl:NamedIndividual ,
                          :Disease ;
                 :hasSymptom :hip_joint_pain ,
                             :joint_pain ,
                             :knee_pain ,
                             :neck_pain ,
                             :painful_walking ,
                             :swelling_joints ;
                 :descrição "Osteoarthritis is the most common form of arthritis, affecting millions of people worldwide. It occurs when the protective cartilage that cushions the ends of your bones wears down over time." .


###  http://www.example.org/disease-ontology#Paralysis_brain_hemorrhage
:Paralysis_brain_hemorrhage rdf:type owl:NamedIndividual ,
                                     :Disease ;
                            :hasSymptom :altered_sensorium ,
                                        :headache ,
                                        :vomiting ,
                                        :weakness_of_one_body_side ;
                            :descrição "Intracerebral hemorrhage (ICH) is when blood suddenly bursts into brain tissue, causing damage to your brain. Symptoms usually appear suddenly during ICH. They include headache, weakness, confusion, and paralysis, particularly on one side of your body." .


###  http://www.example.org/disease-ontology#Patient1
